{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/dbal": "^4.3",
        "doctrine/doctrine-bundle": "^2.18.0",
        "doctrine/doctrine-migrations-bundle": "^3.5.0",
        "doctrine/orm": "^3.5.2",
        "easycorp/easyadmin-bundle": "^4.26.3",
        "knplabs/knp-menu-bundle": "^3.6",
        "knplabs/knp-paginator-bundle": "^6.9.1",
        "knpuniversity/oauth2-client-bundle": "^2.19.0",
        "league/oauth2-google": "^4.0.1",
        "liip/imagine-bundle": "^2.15.0",
        "phpdocumentor/reflection-docblock": "^5.6.3",
        "phpstan/phpdoc-parser": "^2.3",
        "stof/doctrine-extensions-bundle": "^1.14",
        "symfony/asset": "7.3.*",
        "symfony/asset-mapper": "7.3.*",
        "symfony/console": "7.3.*",
        "symfony/doctrine-messenger": "7.3.*",
        "symfony/dotenv": "7.3.*",
        "symfony/expression-language": "7.3.*",
        "symfony/flex": "^2.8.2",
        "symfony/form": "7.3.*",
        "symfony/framework-bundle": "7.3.*",
        "symfony/google-mailer": "7.3.*",
        "symfony/http-client": "7.3.*",
        "symfony/intl": "7.3.*",
        "symfony/mailer": "7.3.*",
        "symfony/mime": "7.3.*",
        "symfony/monolog-bundle": "^3.10",
        "symfony/notifier": "7.3.*",
        "symfony/process": "7.3.*",
        "symfony/property-access": "7.3.*",
        "symfony/property-info": "7.3.*",
        "symfony/runtime": "7.3.*",
        "symfony/security-bundle": "7.3.*",
        "symfony/serializer": "7.3.*",
        "symfony/stimulus-bundle": "^2.30",
        "symfony/string": "7.3.*",
        "symfony/translation": "7.3.*",
        "symfony/twig-bundle": "7.3.*",
        "symfony/ux-icons": "^2.30",
        "symfony/ux-turbo": "^2.30",
        "symfony/validator": "7.3.*",
        "symfony/web-link": "7.3.*",
        "symfony/yaml": "7.3.*",
        "symfonycasts/reset-password-bundle": "^1.23.2",
        "symfonycasts/verify-email-bundle": "^1.17.4",
        "twig/extra-bundle": "^2.12|^3.21",
        "twig/twig": "^2.12|^3.21.1",
        "vich/uploader-bundle": "^2.8.1"
    },
     "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^4.2",
        "friendsofphp/php-cs-fixer": "^3.88.2",
        "phpstan/phpstan": "^2.1.31",
        "phpstan/phpstan-doctrine": "^2.0.10",
        "phpstan/phpstan-strict-rules": "^2.0.7",
        "phpunit/phpunit": "^12.4.1",
        "rector/rector": "^2.2.3",
        "symfony/browser-kit": "7.3.*",
        "symfony/css-selector": "7.3.*",
        "symfony/debug-bundle": "7.3.*",
        "symfony/maker-bundle": "^1.64",
        "symfony/phpunit-bridge": "^7.3.4",
        "symfony/stopwatch": "7.3.*",
        "symfony/web-profiler-bundle": "7.3.*"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "bump-after-update": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "importmap:install": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "static-code-check": [
            "@cs-fixer:check",
            "@phpstan:check",
            "@rector:check",
            "@phpunit"
        ],
        "cs-fixer:fix": "php vendor/bin/php-cs-fixer fix",
        "cs-fixer:check": "php vendor/bin/php-cs-fixer fix --dry-run --diff",
        "phpstan:check": "phpstan analyse",
        "rector:check": "php vendor/bin/rector process --dry-run",
        "rector:fix": "php vendor/bin/rector process",
        "phpunit": "php bin/phpunit"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "7.3.*"
        }
    }
}
